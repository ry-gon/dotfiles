urxvt.saveLines: 1000000

URxvt*scrollBar: false
!URxvt.font: xft:Inconsolata:size=11.5

!URxvt.font: xft:inconsolata:pixelsize=15:antialias=true:hinting=true, xft:dejavu sans mono:pixelsize=13:antialias=true,hinting=true, xft:Inconsolata\ for\ Powerline:size=15:antialias=true
URxvt.font: xft:Termsyn-Powerline:pixelsize=14, xft:Inconsolata for Powerline

!URxvt.font: "-*-terminus-medium-*-normal-*-12-120-72-72-c-60-*-*"

URxvt.letterSpace: 0

URxvt.keysym.M-Escape: perl:keyboard-select:activate

Xft.lcdfilter: lcddefault
Xft.dpi: 96
Xft.hinting: 0
Xft.hintstyle: hintnone
Xft.rgba: rgb
